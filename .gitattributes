# enforce linux file endings for text files
* text=auto eol=lf

# force binary files to be tracked with git lfs
*.dbg filter=lfs diff=lfs merge=lfs -text
*.dcp filter=lfs diff=lfs merge=lfs -text
*.exe filter=lfs diff=lfs merge=lfs -text
*.mem filter=lfs diff=lfs merge=lfs -text
*.obj filter=lfs diff=lfs merge=lfs -text
*.pb filter=lfs diff=lfs merge=lfs -text
*.reloc filter=lfs diff=lfs merge=lfs -text
*.rtti filter=lfs diff=lfs merge=lfs -text
*.sdb filter=lfs diff=lfs merge=lfs -text
*.svtype filter=lfs diff=lfs merge=lfs -text
*.type filter=lfs diff=lfs merge=lfs -text
*.vdb filter=lfs diff=lfs merge=lfs -text
*.wdb filter=lfs diff=lfs merge=lfs -text
*.xdbg filter=lfs diff=lfs merge=lfs -text